{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Generate Proto",
            "type": "shell",
            "command": "protoc",
            "args": [
                "--go_out=.",
                "--go_opt=paths=source_relative",
                "--go-grpc_out=.",
                "--go-grpc_opt=paths=source_relative",
                "forms-api/proto/forms.proto"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$go"
            ]
        }
    ]
}
